{
  "name": "virtual_background_generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "run-p build:*",
    "build:api": "aspida",
    "build:next": "next build && next export",
    "_build:api--swagger": "npx openapi2aspida",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "test": "jest",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.ts\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --write \"./**/*.ts\"",
    "netlify:setup": "netlify login && netlify link",
    "netlify:deploy": "netlify deploy",
    "netlify:deploy--prod": "npm run build:next && netlify deploy --prod"
  },
  "dependencies": {
    "@aspida/axios": "1.6.3",
    "@netlify/plugin-nextjs": "^3.8.0",
    "@types/qrcode": "^1.4.1",
    "axios": "0.21.1",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint-config-next": "^11.0.1",
    "next": "11.0.1",
    "pathpida": "0.14.0",
    "qrcode": "^1.4.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.9.0",
    "react-icons": "^4.2.0",
    "react-query": "3.13.4",
    "recoil": "^0.3.1",
    "sass": "1.32.8",
    "tailwindcss": "^2.2.2"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.34",
    "@types/react": "17.0.15",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.0.6",
    "jest-css-modules": "^2.1.0",
    "netlify-cli": "^6.0.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "4.3.5"
  }
}
